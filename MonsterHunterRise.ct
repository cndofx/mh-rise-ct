              <ID>1337095168</ID>
              <Description>"Add Items To Item Box"</Description>
              <Options moHideChildren="1"/>
              <Color>000000</Color>
              <VariableType>Auto Assembler Script</VariableType>
              <AssemblerScript>// author: cndofx
// calls the game's "add item" function with a custom ID and quantity
// this function is normally called when buying items (and more?)

[ENABLE]
aobscanmodule(AddItems_Call,MonsterHunterRise.exe,4D 85 C0 0F 84 4F 01 00 00 48)
aobscanmodule(GetItem_Call,MonsterHunterRise.exe,48 89 5C 24 08 48 89 6C 24 10 48 89 74 24 18 57 41 54 41 55 41 56 41 57 48 83 EC 20 48 8B 42 18 33 DB 33 FF 45 8B E0 4C 8B EA 48 8B E9 4C 63 78 18)
aobscanmodule(GetFreeSpot_Call,MonsterHunterRise.exe,48 89 6C 24 20 41 56 48 83 EC 20 41 81)
alloc(AddItems,$1000,MonsterHunterRise.exe)
registersymbol(AddItems)

label(AddItemsAmount)
label(AddItemsID)
registersymbol(AddItemsAmount)
registersymbol(AddItemsID)

label(testval1)
registersymbol(testval1)

AddItems:
  sub rsp,48
  mov rcx,[UnkBaseStatic] // mystery value
  mov rcx,[rcx+108]
  mov rcx,[rcx+418]
  mov rdx,[StorageStatic] // zenny/items struct base
  mov rdx,[rdx+80]
  mov r8d,[AddItemsID]
  call GetItem_Call       // get pointer to the item stack with the given ID in storage, returns in RAX

  cmp rax,0
  jne @f
  mov rcx,[UnkBaseStatic] // mystery value
  mov rcx,[rcx+108]
  mov rcx,[rcx+418]
  mov rdx,[StorageStatic] // zenny/items struct base
  mov rdx,[rdx+80]
  mov r8d,[AddItemsID]
  call GetFreeSpot_Call   // get pointer to the next empty item slot, returns in RAX

  @@:
  mov [testval1],rax      // "Item Address" displayed to user
  mov rcx,[UnkBaseStatic] // mystery value
  mov rcx,[rcx+108]
  mov rcx,[rcx+418]
  mov rdx,[StorageStatic] // zenny/items struct base
  mov rdx,[rdx+80]
  mov r8,rax              // pointer to the item in the storage array
  mov r9d,[AddItemsID]    // item ID to add
  mov ebx,[AddItemsAmount]
  mov [rsp+20],ebx        // item quantity to add
  call AddItems_Call
  add rsp,48
  ret

align 4 CC
AddItemsAmount:
  dd #1
AddItemsID:
  dd 04100006
testval1:
  dq 0
 
[DISABLE]

dealloc(*)
unregistersymbol(*)

</AssemblerScript>
