function format_slots(slot1, slot2, slot3)
  local output = {}
  for i = 1, 3, 1 do
    if slot3 &gt; 0 then
      slot3 = slot3 - 1
      output[i] = 3
    elseif slot2 &gt; 0 then
      slot2 = slot2 - 1
      output[i] = 2
    elseif slot1 &gt; 0 then
      slot1 = slot1 - 1
      output[i] = 1
    else
      output[i] = 0
    end
  end
  return output
end

function print_charm(address, friendly)
  friendly = friendly or false
  local address_s = string.format("%X", address)

  local item_type = readInteger("["..address_s.."]+2C")
  local skill_id_1 = readByte("[["..address_s.."]+78]+20")
  local skill_id_2 = readByte("[["..address_s.."]+78]+21")
  local skill_level_1 = readInteger("[["..address_s.."]+80]+20")
  local skill_level_2 = readInteger("[["..address_s.."]+80]+24")

  local slot1 = readByte("[["..address_s.."]+70]+24")
  local slot2 = readByte("[["..address_s.."]+70]+28")
  local slot3 = readByte("[["..address_s.."]+70]+2C")
  local slots = format_slots(slot1, slot2, slot3)

  if friendly then
    print("charm:")
    printf("address = 0x%X", address)
    printf("skill ids: %i, %i", skill_id_1, skill_id_2)
    printf("skill names: %s, %s", skill_names[skill_id_1+1], skill_names[skill_id_2+1])
    printf("skill levels: %i, %i", skill_level_1, skill_level_2)
  else
    printf("%s,%i,%s,%i,%i,%i,%i", skill_names[skill_id_1+1], skill_level_1
                                 , skill_names[skill_id_2+1], skill_level_2
                                 , slots[1], slots[2], slots[3])
  end
end

local equipment_size = readInteger("[[[[StorageStatic]+88]+98]+10]+1C")
